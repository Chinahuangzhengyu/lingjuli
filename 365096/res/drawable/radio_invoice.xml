<?xml version="1.0" encoding="UTF-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
	<item android:state_pressed="true" android:drawable="@drawable/radio_invoice_checked">
        <shape android:shape="rectangle">
		    <size android:width="16dp" android:height="16dp"/>
		</shape>
    </item>
    <item android:state_checked="true" android:drawable="@drawable/radio_invoice_checked">
        <shape android:shape="rectangle">
		    <size android:width="16dp" android:height="16dp"/>
		</shape>
    </item>
    <item android:state_enabled="true" android:drawable="@drawable/radio_invoice_nochecked">
        <shape android:shape="rectangle" >
		    <size android:width="16dp" android:height="16dp"/>
		</shape>
    </item>
    <item >
        <shape android:shape="rectangle" >
		    <size android:width="16dp" android:height="16dp"/>
		</shape>
    </item>
</selector>